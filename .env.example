VITE_APP_NAME="${APP_NAME}"

# Admin User Configuration
# For production, create admin user with: php artisan admin:create
# DO NOT use default credentials in production!

# Ping System Configuration
# Adjust these values based on your system's capacity to prevent slowdown
# Lower values = less system load but slower completion
# Higher values = faster completion but more system load

# Maximum concurrent ping processes (recommended: 20-50)
PING_MAX_CONCURRENT=30

# Delay between batches in milliseconds (recommended: 50-100ms)
PING_BATCH_DELAY_MS=50

# Enable CPU-aware throttling (recommended: true)
PING_CPU_AWARE_THROTTLING=true

# Ping timeout in milliseconds (recommended: 200-500ms)
PING_TIMEOUT_MS=200

# Maximum wait time for batch completion in seconds
PING_MAX_BATCH_WAIT=0.5

# Queue batch size (recommended: 50-100 with multiple workers)
PING_QUEUE_BATCH_SIZE=100

# Recommended number of queue workers (1-2 for low-end, 2-4 for high-end systems)
PING_RECOMMENDED_WORKERS=2
